{
    "name": "Startup tab manager",
    "version": "0.1",
    "manifest_version": 3,
    "author": "Jaedon Heger",
    "background": {
      "service_worker": "tabManager.js"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
      "default_title": "Tab Manager Extension",
      "default_icon": "favicon.png",
      "default_popup": "index.html"
    }
  }